magic numbers
num=int(input("enter number:"))
while num>9:
    sum=0
    while num>0:
        d=num%10
        sum+=d
        num//=10
    num=sum
if num==1:
    print("magic")
else:
    print("not magic")

composite number
a=int(input("enter number:"))
count=0
for i in range(1,a+1,1):
    if a%i==0:
        print(i,end=" ")
        count+=1
if count>2:
    print("composite number")
else:
    print("not composite")

perfect number
a=int(input("enter number:"))
sum=0
for i in range(1,a):
    sum+=i
if sum==a:
    print("perfect number")
else:
    print("not perfect")

