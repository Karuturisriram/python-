dictionaries
student_marks={
    "ram":90,
    "shyam":80,
    "hari":70,
    "ganesh":70,  
}
dict={}
for i in student_marks:
    if student_marks[i]>=80:
        dict[i]="A+"
    elif student_marks[i]>=70:
        dict[i]="B+"
    elif student_marks[i]>=40:
        dict[i]="C+"
    else:
        dict[i]="D+"
print(dict)
