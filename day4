duplicates
L=list(map(int,input("enter list elements))
L1=[]
for i in L:
    if i not in L1:
        L1.append(i)
print(L1)

  odd number 
list=[1,3,5,2,4,3,1,5,2]
list1=[]
for i in list:
    if list.count(i)%2!=0 and i not in list1:
        list1.append(i)
print(list1)

 print any 3 numbers   
list=[8,9,1,2,3,4,5,0]
list.sort()
print(sum(list[:3]))

print even no in desc to ascend and then odd no in acen to desn
list=[8,5,6,9,10,5,3,11,2,1,3,5,5,9,11]
even=[]
odd=[]
for i in list:
    if i%2==0:
        even.append(i)
else:
        odd.append(i)
even.sort(reverse=True)
odd.sort()
rev=even+odd
print(rev)

l=[8,5,6,9,10,5,3,11,2,1,3,5,5,9,11]
l.sort()
l1=[]
for i in l:
    if i%2==0:
        l1.insert(0,i)
    else:
        l1.append(i)
print(l1)

