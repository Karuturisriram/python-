palimdrome
class Solution:
    def isPalindrome(self, x: int) -> bool:
        if(x<0):
            return False
        rev=0
        original=x
        while(x):
            rev=rev*10+x%10
            x//=10
        if(original==rev):
            return True
        else:
            return False

